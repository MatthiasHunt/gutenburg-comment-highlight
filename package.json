{
  "name": "gutenburg-comment-highlight",
  "displayName": "Gutenberg Syntax Highlighter",
  "description": "Syntax Highlighting for Gutenberg Files",
  "icon": "assets/package-icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/MatthiasHunt/gutenburg-comment-highlight"
  },
  "bugs": {
    "url": "https://github.com/MatthiasHunt/gutenburg-comment-highlight/issues"
  },
  "publisher": "matthiashunt",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "grammars": [
      {
        "scopeName": "gutenberg-html.injection",
        "path": "./syntaxes/html-injection.json",
        "injectTo": [
          "text.html.derivative",
          "text.html.php"
        ],
        "embeddedLanguages": {
          "meta.embedded.block.json": "json",
          "meta.embedded.block.php": "php"
        }
      },
      {
        "scopeName": "wordpress-css.injection",
        "path": "./syntaxes/css-injection.json",
        "injectTo": [
          "source.css"
        ]
      }
    ]
  }
}
