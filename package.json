{
  "name": "wordpress-syntax-highlighter",
  "displayName": "WordPress Syntax Highlighter",
  "description": "Full Site Editing and Gutenberg Semantic Highlighting",
  "icon": "assets/wordpress-syntax-highlighter.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/MatthiasHunt/wordpress-syntax-highlighter"
  },
  "galleryBanner": {
    "color": "#23282d",
    "theme": "dark"
  },
  "bugs": {
    "url": "https://github.com/MatthiasHunt/wordpress-syntax-highlighter/issues"
  },
  "author": {
    "name": "Matthias Hunt",
    "email": "matthias.hunt@gmail.com"
  },
  "publisher": "matthiashunt",
  "license": "MIT",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Programming Languages",
    "Themes",
    "Other"
  ],
  "keywords": [
    "Syntax",
    "Highlighter",
    "Highlight",
    "Highlighting",
    "Coloring",
    "WordPress",
    "Gutenberg",
    "Blocks",
    "Full Site Editing",
    "CSS",
    "HTML",
    "PHP",
    "Block Grammar"
  ],
  "contributes": {
    "grammars": [
      {
        "scopeName": "gutenberg-html.injection",
        "path": "./syntaxes/html-injection.json",
        "injectTo": [
          "text.html.derivative"
        ],
        "embeddedLanguages": {
          "meta.embedded.block.json": "json"
        }
      },
      {
        "scopeName": "wordpress-css.injection",
        "path": "./syntaxes/css-injection.json",
        "injectTo": [
          "source.css"
        ]
      },
      {
        "scopeName": "wordpress-php.injection",
        "path": "./syntaxes/php-injection.json",
        "injectTo": [
          "text.html.php"
        ]
      }
    ]
  }
}
